(define (problem raos_keys1)
 (:domain raos_keys)
 (:objects  l1 l2 - light)
 (:init  (and
   (at l1)
   (have_key k0)
   (not_wearing_sunglasses)
   
   (oneof  (opens k0 l1) (opens k0 l2))
   (oneof  (key_at k1 l1) (key_at k1 l2))
   (oneof  (opens k1 l1) (opens k1 l2))
   (or (not (key_at k1 l1)) (opens k1 l2))
   (or (not (key_at k1 l1)) (not (opens k1 l1)))
   (or (not (key_at k1 l2)) (opens k1 l1))
   (or (not (key_at k1 l2)) (not (opens k1 l2)))
   (oneof  (key_at k2 l1) (key_at k2 l2))
   (oneof  (opens k2 l1) (opens k2 l2))
   (or (not (key_at k2 l1)) (opens k2 l2))
   (or (not (key_at k2 l1)) (not (opens k2 l1)))
   (or (not (key_at k2 l2)) (opens k2 l1))
   (or (not (key_at k2 l2)) (not (opens k2 l2)))
   (oneof  (opens k1 l1) (opens k2 l1))
   (oneof  (key_at k1 l1) (key_at k2 l1))
   (oneof  (opens k1 l2) (opens k2 l2))
   (oneof  (key_at k1 l2) (key_at k2 l2))
   (neq l1 l2)
   (neq l2 l1)
  )
 )
 (:goal (and 
          (have_key k1)
          (have_key k2)
        ))
)
